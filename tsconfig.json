{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "lib": ["ES2022", "DOM"],
    //"types": [""],
    "allowSyntheticDefaultImports": false,
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "incremental": true,
    "skipLibCheck": true
  }
}
